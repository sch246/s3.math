data modify storage temp block_store.id set value "minecraft:jack_o_lantern"
execute if block ~ ~ ~ minecraft:jack_o_lantern[facing=north] run data modify storage temp block_store.state.facing set value "north"
execute if block ~ ~ ~ minecraft:jack_o_lantern[facing=south] run data modify storage temp block_store.state.facing set value "south"
execute if block ~ ~ ~ minecraft:jack_o_lantern[facing=west] run data modify storage temp block_store.state.facing set value "west"
execute if block ~ ~ ~ minecraft:jack_o_lantern[facing=east] run data modify storage temp block_store.state.facing set value "east"
