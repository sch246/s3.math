execute if block ~ ~ ~ minecraft:dispenser[facing=up] run data modify storage temp block_store.state.facing set value "up"
execute if block ~ ~ ~ minecraft:dispenser[facing=down] run data modify storage temp block_store.state.facing set value "down"
