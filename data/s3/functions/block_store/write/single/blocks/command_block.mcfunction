execute if block ~ ~ ~ minecraft:command_block[facing=south] run data modify storage temp block_store.state.facing set value "south"
execute if block ~ ~ ~ minecraft:command_block[facing=west] run data modify storage temp block_store.state.facing set value "west"
execute if block ~ ~ ~ minecraft:command_block[facing=up] run data modify storage temp block_store.state.facing set value "up"
